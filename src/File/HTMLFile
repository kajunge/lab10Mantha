<!DOCTYPE html><html class="no-js" lang="en"><head>    <meta charset="utf-8">    <meta http-equiv="x-ua-compatible" content="ie=edge">    <title>Penn State Canvas Login</title>    <meta name="description" content="Canvas Login for Penn State Students, Employees and Friends of Penn State">    <meta name="viewport" content="width=device-width, initial-scale=1">    <script type="text/javascript" src="/CanvasAuth/scripts/jquery-2.2.1.min.js"></script>    <script type="text/javascript" src="/CanvasAuth/scripts/jquery-ui-1.12.0.min.js"></script>    <link rel="stylesheet" href="/CanvasAuth/css/jquery-ui-1.12.0.min.css" />    <link rel="stylesheet" href="/CanvasAuth/css/font-awesome/css/font-awesome.min.css" />    <link rel="stylesheet" href="/CanvasAuth/css/main.css">    <link href="https://fonts.googleapis.com/css?family=Varela+Round" rel="stylesheet" type="text/css">    <script>		function getParameterByName(name, url) {			if (!url) url = window.location.href;			name = name.replace(/[\[\]]/g, "\\$&");			var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),				results = regex.exec(url);			if (!results || !results[2]) return '';			return decodeURIComponent(results[2].replace(/\+/g, " "));		}		function getCurrentUrlWithoutQueryStrings() {		  return window.location.href.split(/[?]/)[0];		}		function getCanvasHostname() {			//var hostname = location.hostname.toLowerCase();			//if (hostname == 'https://lmstoolsaccept.ais.psu.edu') {			//	return 'https://psu.beta.instructure.com'			//} else if (hostname == 'https://lmstools.ais.psu.edu') {			//	return 'https://psu.instructure.com'			//} else if (hostname == 'https://lmstoolsdev.ais.psu.edu') {			//    return 'https://psu.test.instructure.com';			//}			var host = window.location.hostname;			if (host.search("lmstools.ais.psu.edu") != -1) {			    return 'https://psu.instructure.com'			}			else if (host.search("lmstoolsaccept.ais.psu.edu") != -1) {			    return 'https://psu.beta.instructure.com'			}			else if (host.search("lmstoolsdev.ais.psu.edu") != -1) {			    return 'https://psu.test.instructure.com';			}					}		function setTLTNotification() {		    var tltRssFeed = '/api/v1/psuapi/rss';		    $.ajax({		        url: tltRssFeed,		        dataType: 'json',		        success: function (notification) {		            if (notification == '') {		                return;		            }		            var notificationCtrl = $("#tltNotification");		            var posMy, posAt;		            notificationCtrl.find('article').html(notification);		            if (window.innerWidth > 680) {		                posMy = "center center";		                posAt = "top+20%";		            }		            if (notificationCtrl.is(':ui-dialog')) {		                notificationCtrl.dialog('destroy');		            }		            notificationCtrl.dialog({		                width: '40%',		                position: { my: posMy, at: posAt },		                dialogClass: 'notification-dialog',		                autoOpen: true		            });		        }		    });		}		function setPsuMemberAuthenticationLink() {		    var authenticationProvider = getParameterByName('auth_provider').toLowerCase();		    if (authenticationProvider === '') {		        // No authentication provider specified, redirect to Canvas		        window.location.href = getCanvasHostname();		    }		    var membersBtnText = '',				membersBtnUrl = '';		    switch (authenticationProvider) {		        case 'ldap':		            membersBtnText = 'Penn State LDAP';		            membersBtnUrl = getCanvasHostname() + '/login/ldap?expires=' + (Date.now() + 3600000);		            break;		        default:		            membersBtnText = 'Standard Access Account';		            membersBtnUrl = getCanvasHostname() + '/login/saml/1080';		            break;		    }		    // Set 'Faculty, Staff, & Students' text and url		    var membersBtn = $('#members');		    membersBtn.attr('href', membersBtnUrl);		    membersBtn.find('h3').html(membersBtnText);		}		function setFPSAuthenticationLink() {		    // Set 'Friends of Penn State' button url		    $('#friends').attr('href', getCanvasHostname() + '/login/saml/1971');		}		$(function () {		    setPsuMemberAuthenticationLink();		    setFPSAuthenticationLink();		    setTLTNotification();		    // Strip the authentication provider from the browser's URL.		    // If the end-user bookmarks this link, they will be redirected back to Canvas where the most recent authentication provider will be retrieved.		    history.pushState(null, "Canvas Login", getCurrentUrlWithoutQueryStrings());		});		$(window).resize(function () {		    setTLTNotification();		});    </script>    <style>        .ui-widget.notification-dialog {            font-size: 0.9em;        }        .ui-widget-content.notification-dialog {            border: 1px solid #ff9933;        }        .ui-dialog.notification-dialog .ui-widget-header {            background: #ff9933;            border: 0;            color: #fff;        }        .ui-dialog.notification-dialog .notification-icon {            color: #ff9933;            margin-top: 5%;            width: 5%;            float: left;        }        .ui-dialog.notification-dialog .notification-content {            margin-left: 7%;            width: 93%;        }    </style></head><body>    <div id="container">        <header class="psu-mark-region">            <div class="header-content">                <a href="http://www.psu.edu/">                    <img src="/img/PS_HOR_REV_RGB_2C_large.png" alt="Penn State" class="psu-logo">                </a>            </div>        </header>        <section class="login-area">            <div class="login-content">                <div>                    <h1>                        canvas<br>                        <span class="canvasTitleTagline">learning management system</span>                    </h1>                </div>                <h2>Please select a login option.</h2>                <div class="login-links">                    <div class="link-block left">                        <a id="members" class="rounded-button" href="#"><h3></h3></a>                        <span class="link-description">Faculty, Staff, &amp; Students</span>                    </div>                    <div class="link-block right">                        <a id="friends" class="rounded-button" href="#"><h3>Limited Access Account</h3></a>                        <span class="link-description">Limited account must be enrolled in a course</span>                    </div>                    <div class="clearfix"></div>                </div>            </div>        </section>        <div id="tltNotification" title="Canvas Alert" style="display: none;" ClientIDMode="static" runat="server"><article></article></div>        <footer>            <div class="copyright">Copyright 2018 &copy; The Pennsylvania State University</div>            <ul class="footer-links">                <li><a href="http://www.psu.edu/web-privacy-statement">Privacy</a></li>                <li><a href="http://guru.psu.edu/policies/AD85.html">Non-Discrimination</a></li>                <li><a href="http://guru.psu.edu/policies/OHR/hr11.html">Equal Opportunity</a></li>                <li><a href="http://www.psu.edu/accessibilitystatement">Accessibility</a></li>                <li><a href="http://www.psu.edu/legal-statements">Legal</a></li>            </ul>        </footer>    </div></body></html>